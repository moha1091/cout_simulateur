{
  "functions": {
    "api/*.py": {
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.py" }
  ],
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": "api",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ]
}
